{
    "name": "icecube/composer-npm",
    "description": "Composer Plugin for installing NPM packages from a composer.json",
    "homepage": "https://github.com/icecubenz-open-mage/composer-npm",
    "license": "BSD-2-Clause",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "extra": {
        "class": "Icecube\\ComposerNpm\\Plugin",
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Icecube\\ComposerNpm\\": "Icecube/ComposerNpm"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icecube\\ComposerNpm\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
